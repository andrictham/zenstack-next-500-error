{
  "name": "spike-zenstack-clerk-approuter",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=20.12.2"
  },
  "packageManager": "pnpm@9.1.2",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "postinstall": "pnpm db:generate",
    "lint": "next lint",
    "type-check": "npx tsc --noemit",
    "check": "pnpm lint && pnpm type-check && pnpm exec zenstack check",
    "format": "prettier --write \"**/*.{js,cjs,mjs,ts,tsx,md,json}\" --ignore-path .gitignore",
    "start": "next start",
    "db:seed": "tsx prisma/seed/seed.ts",
    "db:generate": "pnpm exec zenstack generate",
    "db:push": "pnpm prisma db push --skip-generate",
    "db:hard-push": "pnpm prisma db push --skip-generate --accept-data-loss",
    "db:migrate:dev": "pnpm prisma migrate dev",
    "db:migrate:dev:create-only": "pnpm prisma migrate dev --create-only",
    "db:migrate:prod": "pnpm prisma migrate deploy",
    "db:studio": "npx prisma studio"
  },
  "dependencies": {
    "@clerk/backend": "1.13.1",
    "@clerk/nextjs": "5.5.5",
    "@clerk/themes": "2.1.30",
    "@emotion/react": "11.13.3",
    "@emotion/server": "11.11.0",
    "@mantine/core": "7.12.2",
    "@mantine/form": "7.12.2",
    "@mantine/hooks": "7.12.2",
    "@mantine/next": "^6.0.21",
    "@prisma/client": "5.19.1",
    "@t3-oss/env-nextjs": "0.11.1",
    "@tabler/icons-react": "3.17.0",
    "@tanstack/react-query": "4.36.1",
    "@zenstackhq/runtime": "2.5.1",
    "@zenstackhq/server": "2.5.1",
    "cuid": "3.0.0",
    "nanoid": "5.0.7",
    "next": "14.2.10",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "superjson": "2.2.1",
    "zod": "^3.23.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
    "@next/env": "14.2.12",
    "@types/eslint": "^8.56.10",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.12.7",
    "@types/react": "18.3.7",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@zenstackhq/tanstack-query": "2.5.1",
    "ap-style-title-case": "^2.0.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.3",
    "js-yaml": "^4.1.0",
    "postcss": "8.4.47",
    "postcss-preset-mantine": "1.17.0",
    "postcss-simple-vars": "7.0.1",
    "prettier": "3.3.3",
    "prisma": "5.19.1",
    "stylelint": "16.9.0",
    "stylelint-config-standard-scss": "13.1.0",
    "tsx": "4.19.1",
    "typescript": "5.6.2",
    "zenstack": "2.5.1"
  },
  "ct3aMetadata": {
    "initVersion": "7.15.0"
  }
}
