{
  "name": "spike-zenstack-clerk-approuter",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=20.12.2"
  },
  "packageManager": "pnpm@9.1.2",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "postinstall": "pnpm db:generate",
    "lint": "next lint",
    "type-check": "npx tsc --noemit",
    "check": "pnpm lint && pnpm type-check",
    "format": "prettier --write \"**/*.{js,cjs,mjs,ts,tsx,md,json}\" --ignore-path .gitignore",
    "start": "next start",
    "db:start": "pnpx supabase start --exclude gotrue,realtime,storage-api,imgproxy,kong,inbucket,postgrest,pgadmin-schema-diff,migra,postgres-meta,studio,edge-runtime,logflare,vector",
    "db:stop": "pnpx supabase stop --backup",
    "db:seed": "tsx prisma/seed/seed.ts",
    "db:generate": "pnpm exec zenstack generate",
    "db:push": "pnpm prisma db push --skip-generate",
    "db:hard-push": "pnpm prisma db push --skip-generate --accept-data-loss",
    "db:migrate:dev": "pnpm prisma migrate dev",
    "db:migrate:dev:create-only": "pnpm prisma migrate dev --create-only",
    "db:migrate:prod": "pnpm prisma migrate deploy",
    "db:studio": "npx prisma studio",
    "auth:set-admin": "sh scripts/set-admin.sh $npm_config_user_id"
  },
  "dependencies": {
    "@clerk/backend": "^1.1.0",
    "@clerk/nextjs": "^5.0.3",
    "@clerk/themes": "^2.1.0",
    "@emotion/react": "^11.11.4",
    "@emotion/server": "^11.11.0",
    "@mantine/core": "^7.8.1",
    "@mantine/form": "^7.8.1",
    "@mantine/hooks": "^7.8.1",
    "@mantine/next": "^6.0.21",
    "@prisma/client": "^5.13.0",
    "@t3-oss/env-nextjs": "^0.10.1",
    "@tabler/icons-react": "^3.2.0",
    "@tanstack/react-query": "^4.36.1",
    "@zenstackhq/runtime": "^2.1.2",
    "@zenstackhq/server": "^2.1.2",
    "cuid": "^3.0.0",
    "nanoid": "^5.0.7",
    "next": "14.2.8",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "superjson": "2.2.1",
    "svix": "^1.21.0",
    "zod": "^3.23.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
    "@next/env": "^14.2.3",
    "@types/browser-sync-webpack-plugin": "^2.2.5",
    "@types/eslint": "^8.56.10",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.12.7",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@zenstackhq/tanstack-query": "^2.1.2",
    "ap-style-title-case": "^2.0.0",
    "browser-sync-webpack-plugin": "^2.3.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.3",
    "js-yaml": "^4.1.0",
    "postcss": "^8.4.38",
    "postcss-preset-mantine": "^1.15.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.2.5",
    "prisma": "^5.13.0",
    "stylelint": "^16.4.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "tsx": "^4.7.3",
    "typescript": "^5.4.5",
    "zenstack": "^2.1.2"
  },
  "ct3aMetadata": {
    "initVersion": "7.15.0"
  }
}
